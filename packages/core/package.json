{
  "name": "@mytangerine/core",
  "version": "0.1.0",
  "description": "myTangerine 핵심 비즈니스 로직",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "order-management",
    "google-sheets",
    "typescript"
  ],
  "author": "fomalhaut84",
  "license": "MIT",
  "dependencies": {
    "date-fns": "^4.1.0",
    "dotenv": "^16.3.0",
    "google-auth-library": "^10.5.0",
    "googleapis": "^167.0.0",
    "lodash": "^4.17.21",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.20",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^4.0.15",
    "eslint": "^8.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^4.0.15"
  }
}
