    "rest-api",
    "order-management"
  ],
  "author": "fomalhaut84",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^9.0.0",
    "@fastify/swagger": "^9.6.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@mytangerine/core": "workspace:*",
    "dotenv": "^16.3.0",
    "fastify": "^4.0.0",
    "fastify-plugin": "^4.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.0.0",
    "pino-pretty": "^11.0.0",
    "tsup": "^8.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  }
